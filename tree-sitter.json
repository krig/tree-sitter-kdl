{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "kdl",
      "camelcase": "KDL",
      "title": "KDL",
      "scope": "source.kdl",
      "path": ".",
      "file-types": [
        "kdl"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injections": [
        "queries/injections.scm"
      ],
      "locals": [
        "queries/locals.scm"
      ]
    }
  ],
  "metadata": {
    "version": "0.2.0",
    "license": "MIT",
    "description": "Tree-sitter Grammar for KDL",
    "authors": [
      {
        "name": "Amaan Qureshi"
      },
      {
        "name": "Andrew Hlynskyi"
      },
      {
        "name": "Kristoffer Gr√∂nlund"
      }
    ],
    "links": {
      "repository": "https://github.com/krig/tree-sitter-kdl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": true
  }
}
